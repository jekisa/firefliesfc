.countdownDigitWrapper {
  background: linear-gradient(135deg, rgba(0, 0, 0, 0.4) 0%, rgba(0, 0, 0, 0.2) 100%);
  border: 3px solid rgba(217, 4, 41, 0.4);
  border-radius: 20px;
  padding: 1.5rem 2rem;
  min-width: 130px;
  box-shadow: 0 15px 40px rgba(0, 0, 0, 0.5),
              inset 0 2px 4px rgba(255, 255, 255, 0.1);
  position: relative;
  overflow: hidden;
  transition: all 0.3s ease;
}

.countdownDigitWrapper::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 50%;
  background: linear-gradient(to bottom, rgba(255, 255, 255, 0.1) 0%, transparent 100%);
  pointer-events: none;
}

.countdownDigitWrapper:hover {
  border-color: rgba(217, 4, 41, 0.7);
  box-shadow: 0 20px 50px rgba(217, 4, 41, 0.3),
              inset 0 2px 4px rgba(255, 255, 255, 0.15);
  transform: translateY(-5px);
}

.countdownDigit {
  font-size: 4rem;
  font-weight: 900;
  color: white;
  line-height: 1;
  font-family: 'Courier New', monospace;
  letter-spacing: 2px;
  text-shadow: 0 4px 20px rgba(217, 4, 41, 0.6),
               0 0 40px rgba(217, 4, 41, 0.4);
  animation: digitGlow 2s ease-in-out infinite;
}

@keyframes digitGlow {
  0%, 100% {
    text-shadow: 0 4px 20px rgba(217, 4, 41, 0.6),
                 0 0 40px rgba(217, 4, 41, 0.4);
  }
  50% {
    text-shadow: 0 4px 30px rgba(217, 4, 41, 0.8),
                 0 0 60px rgba(217, 4, 41, 0.6);
  }
}

.countdownLabel2 {
  font-size: 0.95rem;
  font-weight: 600;
  color: rgba(255, 255, 255, 0.7);
  text-transform: uppercase;
  letter-spacing: 2px;
}

.countdownSeparator {
  font-size: 3.5rem;
  font-weight: 900;
  color: rgba(217, 4, 41, 0.8);
  animation: blink 1s ease-in-out infinite;
  text-shadow: 0 0 30px rgba(217, 4, 41, 0.8);
}

@keyframes blink {
  0%, 100% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
}

/* Next Match Section Responsive */
@media (max-width: 1200px) {
  .teamsMatchup {
    gap: 3rem;
  }

  .teamLogoWrapper {
    width: 150px;
    height: 150px;
  }

  .teamName {
    font-size: 1.75rem;
  }

  .teamDescription {
    font-size: 0.95rem;
  }

  .countdownScroll {
    gap: 2rem;
  }

  .countdownDigitWrapper {
    min-width: 110px;
    padding: 1.25rem 1.5rem;
  }

  .countdownDigit {
    font-size: 3.5rem;
  }
}

@media (max-width: 768px) {
  .nextMatchSection {
    padding: 4rem 0;
  }

  .nextMatchHeader {
    margin-bottom: 3rem;
  }

  .nextMatchTitle {
    font-size: 2.5rem;
  }

  .nextMatchContent {
    gap: 2.5rem;
  }

  .opponentCard {
    padding: 3rem 2rem;
  }

  .teamsMatchup {
    grid-template-columns: 1fr;
    gap: 3rem;
  }

  .vsSection {
    order: -1;
  }

  .vsLine {
    width: 120px;
    height: 2px;
    background: linear-gradient(
      to right,
      transparent 0%,
      rgba(217, 4, 41, 0.6) 20%,
      rgba(217, 4, 41, 0.8) 50%,
      rgba(217, 4, 41, 0.6) 80%,
      transparent 100%
    );
  }

  .vsLine::before {
    top: 50%;
    left: -5px;
    transform: translateY(-50%);
  }

  .vsLine::after {
    top: 50%;
    right: -5px;
    left: auto;
    transform: translateY(-50%);
  }

  .vsIndicator {
    width: 80px;
    height: 80px;
    font-size: 1.75rem;
  }

  .teamLogoWrapper {
    width: 140px;
    height: 140px;
  }

  .teamName {
    font-size: 1.5rem;
  }

  .teamDescription {
    font-size: 0.9rem;
  }

  .locationCard {
    flex-direction: column;
    padding: 2rem 1.5rem;
    gap: 1.5rem;
    text-align: center;
  }

  .locationIcon {
    width: 60px;
    height: 60px;
  }

  .locationIcon svg {
    width: 24px;
    height: 24px;
  }

  .locationText {
    font-size: 1rem;
  }

  .locationButton {
    padding: 0.875rem 1.75rem;
    font-size: 0.95rem;
  }

  .countdownScrollWrapper {
    padding: 2.5rem 2rem;
  }

  .countdownScroll {
    gap: 1.5rem;
  }

  .countdownDigitWrapper {
    min-width: 90px;
    padding: 1rem 1.25rem;
  }

  .countdownDigit {
    font-size: 3rem;
  }

  .countdownLabel2 {
    font-size: 0.85rem;
  }

  .countdownSeparator {
    font-size: 2.5rem;
  }
}

@media (max-width: 480px) {
  .nextMatchSection {
    padding: 3rem 0;
  }

  .nextMatchContainer {
    padding: 0 1.5rem;
  }

  .nextMatchBadge {
    padding: 0.6rem 1.5rem;
    font-size: 0.85rem;
  }

  .nextMatchTitle {
    font-size: 2rem;
  }

  .opponentCard {
    padding: 2rem 1.5rem;
  }

  .teamsMatchup {
    gap: 2.5rem;
  }

  .vsIndicator {
    width: 70px;
    height: 70px;
    font-size: 1.5rem;
  }

  .vsLine {
    width: 100px;
  }

  .teamLogoWrapper {
    width: 120px;
    height: 120px;
    padding: 1.5rem;
  }

  .teamName {
    font-size: 1.25rem;
  }

  .teamDescription {
    font-size: 0.85rem;
  }

  .locationCard {
    padding: 1.5rem 1rem;
    gap: 1.25rem;
  }

  .locationIcon {
    width: 50px;
    height: 50px;
  }

  .locationIcon svg {
    width: 20px;
    height: 20px;
  }

  .locationText {
    font-size: 0.9rem;
  }

  .locationButton {
    padding: 0.75rem 1.5rem;
    font-size: 0.9rem;
  }

  .locationButton svg {
    width: 16px;
    height: 16px;
  }

  .countdownLabel {
    font-size: 1.1rem;
    margin-bottom: 1.5rem;
  }

  .countdownScrollWrapper {
    padding: 2rem 1.5rem;
  }

  .countdownScroll {
    gap: 1rem;
    flex-wrap: wrap;
  }

  .countdownDigitWrapper {
    min-width: 70px;
    padding: 0.875rem 1rem;
  }

  .countdownDigit {
    font-size: 2.25rem;
  }

  .countdownLabel2 {
    font-size: 0.75rem;
  }

  .countdownSeparator {
    font-size: 2rem;
    display: none;
  }
}

/* Partners Section Responsive */
@media (max-width: 768px) {
  .partnersSection {
    padding: 3rem 0;
  }

  .partnersTitle {
    font-size: 2rem;
  }

  .partnersSubtitle {
    font-size: 1rem;
    margin-bottom: 2.5rem;
  }

  .partnerCard {
    min-width: 160px;
    max-width: 280px;
    height: 100px;
    padding: 1.25rem 2rem;
    gap: 1rem;
  }

  .partnerIcon {
    width: 40px;
    height: 40px;
  }

  .partnerLogo {
    font-size: 1.15rem;
  }

  .carouselNavigation {
    margin-top: 2rem;
  }

  .navDot {
    width: 10px;
    height: 10px;
  }
}

@media (max-width: 480px) {
  .partnersSection {
    padding: 2.5rem 0;
  }

  .partnersSection::before,
  .partnersSection::after {
    font-size: 5rem;
  }

  .partnersTitle {
    font-size: 1.75rem;
  }

  .partnersSubtitle {
    font-size: 0.95rem;
    padding: 0 1rem;
    margin-bottom: 2rem;
  }

  .partnerCard {
    min-width: 140px;
    max-width: 240px;
    height: 90px;
    padding: 1rem 1.5rem;
    gap: 0.75rem;
  }

  .partnerIcon {
    width: 35px;
    height: 35px;
  }

  .partnerLogo {
    font-size: 1rem;
  }

  .partnersTrack {
    gap: 1.5rem;
  }

  .carouselNavigation {
    margin-top: 1.5rem;
    gap: 0.5rem;
  }

  .navDot {
    width: 8px;
    height: 8px;
  }
}